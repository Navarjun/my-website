extends design-header

block content
  .row.portfolio-container
    each project in projects
      .col-md-4.col-sm-6
        a(href="/design/"+project.urlId)
          .col-sm-9.col-xs-offset-1.col-xs-9.card
            img(src=""+project.imageFile, style="width: 100%;")
            p= project.title
